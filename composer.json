{
    "name": "must/rr-logger",
    "description": "Request and Response Logger for API Projects",
    "type": "library",
    "authors": [
        {
            "name": "Promise Mphoola",
            "email": "promisemphoola2@gmail.com"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "^11.2",
        "orchestra/testbench": "^9.2"
    },
    "license": "MIT",
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "MUST\\RRLogger\\": "src/",
            "MUST\\RRLogger\\Tests\\": "tests/",
            "MUST\\RRLogger\\Database\\Factories\\": "database/factories/"
        }
    },
    "require": {
        "php": "^8.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "MUST\\RRLogger\\Providers\\RRLoggerServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-f": "vendor/bin/phpunit --filter"
    }
}